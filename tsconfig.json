{ "include":["./src/lib/Dev/*"],
  "compilerOptions": {
    "module": "commonjs",
    "target": "ESNext",
    "removeComments": true,
    "alwaysStrict":true,
    "moduleResolution": "node",
    "declaration": true,
    "esModuleInterop": true,
    "strict": true,
    "sourceMap": false,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "importsNotUsedAsValues":"remove",
    "skipLibCheck": true
  }
}
